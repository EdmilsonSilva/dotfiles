[user]
	email = ${GIT_EMAIL}
	name = ${GIT_NAME}
[alias]
    s = status
    br = branch
    ck = checkout
    co = commit
    unstage = reset HEAD --
    lg = log --graph --abbrev-commit --decorate --format=fomrat:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
    email = config --get-all user.email
    name = config --get-all user.name
[pull]
	rebase = false
[core]
	autocrlf = input
	editor = vim
[url "git@bitbucket.org:"]
	insteadOf = https://bitbucket.org/
